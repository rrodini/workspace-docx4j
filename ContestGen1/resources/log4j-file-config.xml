    <Logger name="com.rodini.contestgen.common"> 
    </Logger>
    <Logger name="com.rodini.contestgen.extract"> 
    </Logger>
    <Logger name="com.rodini.contestgen.generate"> 
    </Logger>
    <Logger name="com.rodini.contestgen.model"> 
    </Logger>
  </Loggers>
</Configuration>