<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="BallotNamer" packages="">
  <Appenders>
    <File name="BallotNamer" fileName="../logs/ballotnamer.log">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    </File>
  </Appenders>
  <Loggers>
    <Root level="ERROR">
      <AppenderRef ref="BallotNamer"/>
    </Root>
  </Loggers>
</Configuration>