<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="BallotZipper" packages="">
  <Appenders>
    <File name="BallotZipper" fileName="../logs/ballotzipper.log">
      <PatternLayout pattern="%-5level %c{1}:%M:%L - %msg%n"/>
	  <Append>false</Append>		
    </File>
  </Appenders>
  <Loggers>
    <Root level="ERROR">
      <AppenderRef ref="BallotZipper"/>
    </Root>
   <Logger name="com.rodini.ballotzipper"> 
    </Logger>
  </Loggers>
</Configuration>