<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="warn" name="TextCleaner" packages="">
  <Appenders>
    <File name="TextCleaner" fileName="../logs/textcleaner.log">
      <PatternLayout pattern="%-5level %c{1}:%M:%L - %msg%n"/>
    </File>
  </Appenders>
  <Loggers>
    <Root level="ERROR">
      <AppenderRef ref="TextCleaner"/>
    </Root>
    <Logger name="com.rodini.textcleaner"> 
    </Logger>
  </Loggers>
</Configuration>